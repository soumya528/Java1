import java.io.*;
import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int totalMarks = 0;
        for (int i = 0; i < n; i++) {
            int marks = scanner.nextInt();
            totalMarks += marks;
        }
        int average = totalMarks / n;
        System.out.println(average);
    }
}