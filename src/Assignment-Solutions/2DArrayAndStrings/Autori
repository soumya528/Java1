import java.io.*;
import java.util.*;
public class Main {
    public static void main(String args[]) {
        // your code here
        Scanner sc = new Scanner(System.in);
        String str = sc.next();
        int n = str.length(),i,j;
        //We have this line....
        
        System.out.print(str.charAt(0));
        for(i=1;i<n;i++){

            if(str.charAt(i)=='-')
                System.out.print(str.charAt(i+1));
        }
        System.out.println();
    }
}