import java.util.*;
import java.io.*;

public class Main {
    public static void main(String args[]) {

        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int i,j;
        int[] arr = new int[n];
        
        for(i=0;i<n;i++){
            arr[i] = sc.nextInt();
        }
        int size = arr.length;
        Arrays.sort(arr);
        
        int res = arr[1];    
        System.out.println(res);
    }
}